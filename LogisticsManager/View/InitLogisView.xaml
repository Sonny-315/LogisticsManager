<Window x:Class="LogisticsManager.View.InitLogisView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LogisticsManager.View"
        mc:Ignorable="d"
        Title="Initialize Logistics Grid" Height="400" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            
        </Grid>
        <Grid Grid.Row="1">

        </Grid>
        <Grid Grid.Row="2">

        </Grid>
        <Grid Grid.Row="3">
            <WrapPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                <Button Command="{Binding CmdLogisGridSave}" Content="Save" Width="70" Height="40"/>
                <Button Command="{Binding CmdLogisGridCancel}" Content="Cancel" Width="70" Height="40" Margin="10 0 0 0"/>
            </WrapPanel>
            
            
        </Grid>



    </Grid>
</Window>
